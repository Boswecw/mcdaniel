        "@types/node": "^24.3.0",
        "@vitest/browser": "^3.2.4",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-svelte": "^2.30.0",
        "playwright": "^1.55.0",
        "prettier": "^3.6.2",
